{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "isolatedModules": true,
    "lib": ["esnext"],
    "module": "CommonJS",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "es6"
  },
  "include": ["generate_proptypes/*.ts"]
}
